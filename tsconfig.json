{
	"compilerOptions": {
		"target": "es5",
		"jsx": "react",
		"module": "commonjs",
		"moduleResolution": "node",
		"outDir": "./build",
		"rootDir": "./src",
		"lib": [
			"ES2015",
			"DOM",
			"DOM.Iterable"
		],
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true
	}
}
